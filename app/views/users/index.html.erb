<main>
  <h2>List of Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>Username</th>
        <th>Votes</th>
        <th>Joined</th>
      </tr>
    </thead>
    <tbody>
    <%@users.each do |user|%>
      <tr>
        <td><%=link_to user.username, user_path(user.id)%></td>
        <td><%=user.votes.length%></td>
        <td><%=user.created_at.strftime("%m/%d/%Y")%></td>
      </tr>
    <%end%>
    </tbody>
  </table>
</main>